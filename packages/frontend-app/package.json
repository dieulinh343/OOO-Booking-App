{
  "name": "frontend-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ooo-booking/commons": "1.0.0",
    "@ahaui/react": "2.1.0",
    "@ahaui/css": "2.1.0"
  },
  "scripts": {
    "start": "node ../../portal-webpack/scripts/start.js",
    "start:dev": "REACT_APP_ENV=dev npm run start",
    "start:staging": "REACT_APP_ENV=staging npm run start",
    "start:prod": "REACT_APP_ENV=prod npm run start",
    "build": "node ../../portal-webpack/scripts/build.js",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
